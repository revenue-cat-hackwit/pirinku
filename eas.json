{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "24.11.1",
      "npm": {
        "install": {
          "args": ["--legacy-peer-deps"]
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "24.11.1",
      "npm": {
        "install": {
          "args": ["--legacy-peer-deps"]
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "node": "24.11.1",
      "npm": {
        "install": {
          "args": ["--legacy-peer-deps"]
        }
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
